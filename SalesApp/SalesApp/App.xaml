<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
			 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
			 xmlns:infra="clr-namespace:SalesApp.Infrastructure"
			 x:Class="SalesApp.App">
	<Application.Resources>
		<ResourceDictionary>
			<!--Locator-->
			<infra:InstanceLocator x:Key="Locator"/>
			<!--Parameters-->
			<x:String x:Key="UrlAPI">https://salesapiservices.azurewebsites.net</x:String>
			<x:String x:Key="UrlAPILocal">http://localhost:54579</x:String>
			<x:String x:Key="UrlPrefix">/api</x:String>
			<x:String x:Key="UrlProductsController">/Products</x:String>
		</ResourceDictionary>
	</Application.Resources>
</Application>